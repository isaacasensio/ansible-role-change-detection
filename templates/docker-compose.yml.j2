# {{ ansible_managed }}
---
version: '3.4'
services:
  change-detection:
    restart: unless-stopped
    pull_policy: always
    image: {{ change_detection_image }}
    container_name: change-detection
    ports:
      - {{change_detection_host_port}}:5000
    user: "{{ created_uid }}:{{ created_gid }}"
    volumes:
      - {{change_detection_host_data_path}}:/datastore

